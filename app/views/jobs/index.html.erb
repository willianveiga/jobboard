<%= render 'backend/shared/menu' %>
<% if not @jobs.empty? %>
  <table>
    <tr>
      <th>Title</th>
      <th>Brief Description</th>
    </tr>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.title %></td>
        <td><%= job.brief_description %></td>
      </tr>
    <% end %>
  </table>
<% else %>
  <p>You don't have any job.</p>
<% end %>
